import java.util.Arrays;

public class main {
    public static void main(String[] args) {
        String input = "aabababbabbaabbbaaaabaabaaabaaaabaaabbaabaabaaabbbbaaaaaabaaaabaaabbaaabaaaabbaaaabaabaabaaabaaaabaaabbaabaabaaabbbbaaaaaabaaaabaaabbaaabaaaabbaaaabaaaabababaaaaababbaabaaabaaaabaaabbaabaabababaaaabaaabababaabbaabaabaabababaaaabaaabbaabaabaaabababaaabaabbaabaa";
        System.out.println(input.length());
        String[] chars = new String[input.length()/2];
        String[] converts = {"aa", "ab", "ba", "bb"};
        int[][] conversions = {{1, 7}, {4, 5}, {8, 2}, {3, 6}};
        for (int i = 0; i < chars.length; i++) {
            chars[i] = input.substring(i*2, i*2+2);
        }
        System.out.println(Arrays.toString(chars));
        String output = "";
        for (String thing : chars) {
            for (int i = 0; i < converts.length; i++) {
                if (thing.equals(converts[i])){
                    if (Math.random() < 0.5)
                    {
                        output += conversions[i][0];
                    } else {
                        output += conversions[i][1];
                    }
                }
            }
        }
        System.out.println(output);
    }
}
